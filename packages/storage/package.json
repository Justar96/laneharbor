{
  "name": "@laneharbor/storage",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "proto:compile": "pbjs -t static-module -w es6 -o src/proto/compiled.js proto/*.proto && pbts -o src/proto/compiled.d.ts src/proto/compiled.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.0",
    "@grpc/grpc-js": "^1.12.5",
    "@grpc/proto-loader": "^0.7.14",
    "dotenv": "^16.4.7",
    "protobufjs": "^7.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "protobufjs-cli": "^1.1.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
